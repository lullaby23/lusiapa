{
  "name": "TG-VC Music BOT",
  "description": "Telegram Voice Chat Music Player Bot.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/lullaby23",
  "logo": "https://telegra.ph/file/0d0888f862c9fad1f1688.jpg",
  "stack": "container",
  "env": {
    "API_ID": {
      "description": "Your API_ID dari my.telegram.org",
      "required": true
    },
    "API_HASH": {
      "description": "Your API_HASH dari my.telegram.org",
      "required": true
    },
    "SESSION_NAME": {
      "description": "Streamer(Dummy) Akun Dummy string session.",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Bot Token Dari @BotFather",
      "required": true
    },
    "BOT_OWNER": {
      "description": "Telegram ID Mu",
      "required": true
    },
    "SUDO_USERS": {
      "description": "ID Admin biar bisa akses admin command",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Limit Durasi Lagu (Detik)",
      "required": false,
      "value": "180"
    },
    "LOG_CHANNEL": {
      "description": "Log Channel, informasi tentang bot",
      "required": true,
      "value": ""
    },
    "DATABASE_URL": {
      "description": "Mongo Db url. Get it from https://www.mongodb.com/",
      "required": true
    },
    "BOT_USERNAME": {
      "description": "Bot username tanpa '@' !",
      "required": true
    },
    "ARQ_API_KEY": {
      "description": "Your ARQ API key! Cari di @ARQRobot !",
      "required": false,
      "value": ""
    },
    "HEROKU_APP_NAME": {
      "description": "Nama Aplikasi Heroku!",
      "required": true,
      "value": ""
    },
    "HEROKU_API_KEY": {
      "description": "Heroku App Key!",
      "required": true,
      "value": ""
    },
    "UPSTREAM_REPO": {
      "description": "Github Repo Url. Kalo pake repo sendiri, klo gk jangan diubah!",
      "required": false,
      "value": "https://github.com/lullaby23/lusiapa"
    },
    "THUMB_URL": {
      "description": "Gambar Thumbnail, upload di telegra.ph",
      "required": false,
      "value": "https://telegra.ph/file/0d0888f862c9fad1f1688.jpg"
    },
    "UPDATES_CHANNEL": {
      "description": "Channel telegram mu tanpa'@' ",
      "required": false,
      "value": "Foryoubbs"
    }
  }
}
